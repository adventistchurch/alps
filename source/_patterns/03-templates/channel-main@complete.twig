<div class="content cf has-aside" role="document">
  {% include 'organisms-header' %}
  <main class="main can-be--dark-dark" role="main">
    {% include 'organisms-page-header' %}
    <div class="layout-container">
      <div class="flex-container cf">
        <div class="shift-left--fluid column__primary bg--white can-be--dark-light">
          {% include 'organisms-body-content' %}
        </div> <!-- /.shift-left--fluid -->
        <div class="shift-right--fluid bg--beige can-be--dark-dark">
          {% if breakout_block %}
            {% include 'molecules-media-block' with {'media_block_default': breakout_block.media_block_default}  %}
          {% endif %}
          <div class="column__secondary can-be--dark-dark">
            {% include 'organisms-aside' %}
          </div>
        </div> <!-- /.shift-right--fluid -->
      </div> <!-- /.flex-container -->
    </div> <!-- /.layout-container -->
  </main> <!-- /.main -->
  {% include 'organisms-footer' %}
</div> <!-- /.content -->
